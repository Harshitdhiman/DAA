#include <iostream>
#include<vector>
using namespace std;
	
int main()
{
    int n;
    
    cin>>n;
    vector<int>nums(n);
    for(int i=0;i<n;i++)
    cin>>nums[i];
    
       int sum=0;
        for(auto val:nums)
            sum+=val;
        int s=sum/2;
        if(sum%2!=0)
            return false;
        
        vector<vector<bool>>dp(nums.size(),vector<bool>(s+1,false));
        for(int i=0;i<nums.size();i++)
            dp[i][0]=true;
        if(nums[0]<=s)
            dp[0][nums[0]]=true;
        for(int i=1;i<nums.size();i++)
        {
            for(int j=1;j<s+1;j++)
            {
                bool no=dp[i-1][j];
                bool t=false;
                if(j>=nums[i])
                    t=dp[i-1][j-nums[i]];
                dp[i][j]=no||t;
                
            }
            
        }
        if(dp[nums.size()-1][s])
        cout<<"Yes";
        else
        cout<<"No";

    return 0;
}

