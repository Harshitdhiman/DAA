#include<stdio.h>
#include<math.h>

int main(){
int n,m;
printf("Enter the no. of element : ");
scanf("%d",&n);
int arr[n];
printf("\nEnter elements : ");

for(int i=0;i<n;i++){
    scanf("%d",&arr[i]);
}
m=sqrt(n);
int k,flag=0;
printf("m= %d",m);

printf("\nEnter key : ");
scanf("%d",&k);
for(int i=0;i<n;i=i+m){
        if(arr[i]==k){
            flag=1;
            break;
        }
    else if(arr[i]>k){
        for(int j=i/m+1;j<i;j++){
            if(arr[j]==k){
                flag=1;
                break;
            }
        }
    }
}
if(flag==1){
    printf("Found");
}

else{
        printf(" Not Found");

}
}
